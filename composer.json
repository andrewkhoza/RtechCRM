{
    "name": "rtech/relevant-technologies",
    "description": "Rtech CRM",
    "keywords": ["rtech", "repairs"],
    "homepage": "https://rtechsa.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap4": "~2.0.6",
        "yiisoft/yii2-symfonymailer": "~3.0.0",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-checkbox-x": "*",
        "2amigos/yii2-tinymce-widget": "~1.1",
        "kartik-v/yii2-grid": "dev-master",
        "wbraganca/yii2-dynamicform": "2.0.2",
        "himiklab/yii2-recaptcha-widget": "*",
        "yurkinx/yii2-image": "^1.2",
        "kolyunya/yii2-map-input-widget" : "*",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev",
        "kartik-v/yii2-mpdf": "dev-master",
        "inquid/yii2-signature": "dev-master",
        "2amigos/2fa-library": "2.0",
        "2amigos/qrcode-library": "2.0",
        "symfony/google-mailer": "^6.3",
        "kartik-v/yii2-export": "*",
        "mk-j/php_xlsxwriter": "^0.39.0",
        "mimicreative/yii2-metismenu": "~1.0",
        "rmrevin/yii2-fontawesome": "~3.5",
        "kartik-v/yii2-icons": "@dev"

    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asser-library": "vendor/bower"
            }
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
